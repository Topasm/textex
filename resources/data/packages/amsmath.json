    "vmatrix": {
      "snippet": "\\\\begin{vmatrix}\n\t$0\n\\\\end{vmatrix}"
    },
    "Vmatrix": {
      "snippet": "\\\\begin{Vmatrix}\n\t$0\n\\\\end{Vmatrix}"
    },
    "smallmatrix": {
