{
  "name": "textex",
  "version": "1.0.0-beta.1",
  "description": "A self-contained desktop LaTeX editor built on Electron",
  "main": "./out/main/main.js",
  "bin": {
    "textex": "./out/cli/cli/index.js"
  },
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "typecheck": "tsc --noEmit",
    "build:cli": "tsc -p tsconfig.cli.json",
    "build:mcp": "tsc -p tsconfig.mcp.json",
    "mcp": "node out/mcp/mcp/server.js",
    "preview": "electron-vite preview",
    "package:win": "electron-vite build && electron-builder --win",
    "package:mac": "electron-vite build && electron-builder --mac",
    "package:linux": "electron-vite build && electron-builder --linux",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "latex",
    "editor",
    "electron",
    "tectonic"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@monaco-editor/react": "^4.7.0",
    "chokidar": "^5.0.0",
    "commander": "^14.0.3",
    "date-fns": "^4.1.0",
    "electron-updater": "^6.3.9",
    "katex": "^0.16.28",
    "lucide-react": "^0.564.0",
    "mathlive": "^0.108.3",
    "monaco-editor": "^0.55.0",
    "nspell": "^2.1.5",
    "pdfjs-dist": "5.4.296",
    "prettier-plugin-latex": "^2.0.1",
    "react": "^19.2.4",
    "react-data-grid": "^7.0.0-beta.59",
    "react-dom": "^19.2.4",
    "react-pdf": "^10.3.0",
    "simple-git": "^3.27.0",
    "vscode-jsonrpc": "^8.2.1",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "dictionary-en": "^4.0.0",
    "electron": "^40.4.1",
    "electron-builder": "^26.7.0",
    "electron-vite": "^5.0.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.2.0",
    "jsdom": "^28.1.0",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.34.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}